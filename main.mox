import "mox/std.mox"
import "mox/array.mox"
import "mox/vector.mox"

function max<T>(a : T, b : T) -> T
    if(a > b)
        return a
    end

    return b
end

function min<T>(a : T, b : T) -> T
    if(a < b)
        return a
    end

    return b
end

function main() -> int
    var i : int = 100
    var j : int = 200

    var k : int = max<int>(i, j)
    var l : int = min<int>(i, j)

    printf("%i > %i", k, l)

    return 0
end
