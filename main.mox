class Pair

    var x : int
    var y : int

    function Pair()
        self.x = 0
        self.y = 0
    end

    function switch()
        var temp : int = self.x
        self.x = self.y
        self.y = temp
    end

    function set(index : int, value : int)
        if(index == 0)
            self.x = value
        else if(index == 1)
            self.y = value
        end
    end
end