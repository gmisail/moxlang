import "mox/std.mox"
import "mox/string.mox"

function max<T>(a : T, b : T) -> T
    var x : T = a

    return x
end

function main() -> int

    printf("%i\n", max<int>(10, 5))
    printf("%f\n", max<float>(10.4, 5.3))
    printf("%f\n", max<double>(10.61, 5.42))

    var a : String = new String("hello")
    var b : String = new String("world")
    
    var c : String = max<String>(a, b)
    printf("%s\n", c.data())

    delete a
    delete b

    return 0
end