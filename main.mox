import "mox/std.mox"
import "mox/vector.mox"
import "mox/string.mox"

function main() -> int
    var str : String = new String("hello world")
    print(str.data())
    delete str

    var vec : Vector = new Vector()

    for(i from 0 to 10)
        vec.add(i)
    end

    for(i from 0 to 10)
        if(i % 2 == 0)
            vec.remove(i)
        end
    end

    for(i from 0 to vec.length())
        printf("%s", vec.get(i))
    end

    delete vec

    return 0
end