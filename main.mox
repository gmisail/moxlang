import "mox/std.mox"
import "mox/array.mox"

function main() -> int
    var arr : Array = new Array(10)
    var sum : int = 0

    for(i from 0 to 10)
        arr.set(i, i)
    end

    for(i from 0 to 10)
        sum = sum + arr.get(i)
    end

    printf("the sum is %i", sum)

    delete arr

    return 0
end