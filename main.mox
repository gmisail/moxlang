class Pair

    var x : int
    var y : int

    function init(x : int, y : int)
        self.x = x
        self.y = y
    end

    function switch()
        var temp : int = self.x
        self.x = self.y
        self.y = temp
    end

    function set(index : int, value : int)
        if(index == 0)
            self.x = value
        else if(index == 1)
            self.y = value
        end
    end
end
