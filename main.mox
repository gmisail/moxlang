import "mox/std.mox"
import "mox/string.mox"
import "point.mox"

function main() -> int
    #
    #   Strings
    #

    var str : String = new String("hello world from moxlang!")
    var another : String = new String("hello world from moxlang!")
    var other : String = new String("bello borld brom boxlang")

    print("this is string number one: ")
    print(str.data())

    print("\nthis is string number two: ")
    print(other.data())

    print("\nthis is string number three: ")
    print(another.data())

    print("\nare string one and two equal? ")

    if(str.equals(other))
        print("yes! \n")
    else
        print("nope! \n")
    end

    print("\nare string one and three equal? ")

    if(str.equals(another))
        print("yes! \n")
    else
        print("nope! \n")
    end

    delete str
    delete another
    delete other

    #
    #   Points
    #

    var i : Point = new Point(0, 0)
    var j : Point = new Point(10, 5)

    i.add(j)

    printf("adding: (%f, %f)\n", i.x, i.y)

    i.sub(j)
    i.sub(j)

    printf("subtracting: (%f, %f)\n", i.x, i.y)

    delete i
    delete j
end


